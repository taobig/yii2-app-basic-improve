{
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "globals": {
    "angular": false
  },
  "rules": {
    "no-console":0,
    "camelcase": 2,
    "curly": 2,
    "brace-style": [2, "1tbs"],
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "space-in-parens": ["error","never"],
//    "space-in-brackets": [2, "never"],
    "space-infix-ops": 2
  },
  "extends": "eslint:recommended"
}
