{
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "globals": {
    "angular": false,
    "jQuery": true,
    "$": true,
    "console": true
  },
  "rules": {
    "no-console":0,
    "camelcase": 2,
    "curly": 2,
    "brace-style": [2, "1tbs"],
    //single  double  backtick-倒引号
    //如果违反了规则情况下，这里的数字：0表示不不处理，1表示警告，2表示错误并退出
//    "quotes": [2, "single"],
    "semi": [2, "always"],
    "space-in-parens": ["error","never"],
//    "space-in-brackets": [2, "never"],
    "space-infix-ops": 2
  },
  "extends": "eslint:recommended"
}
